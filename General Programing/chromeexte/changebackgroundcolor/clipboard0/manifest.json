{
  "name": "Reader Extensions",
  "description" : "Base Level Extension",
  "permissions": ["activeTab", "tabs", "declarativeContent", "storage"],
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "reading.html",
    "default_icon": "read.png"
  },
  "commands": {
  "_execute_browser_action": {
    "suggested_key": {
      "default": "Ctrl+Shift+F",
      "mac": "MacCtrl+Shift+F"
    },
    "description": "Opens reading.html"
  }
}
}
